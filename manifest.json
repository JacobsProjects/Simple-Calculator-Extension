{
  "manifest_version": 3,
  "name": "Simple-Calculator-Extension",
  "version": "1.0.0",
  "description": "A very simple calculator extension",
  "action": {
    "default_icon": "imgs/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "imgs/icon.png",
    "48": "imgs/icon.png",
    "128": "imgs/icon.png"
  },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
}
